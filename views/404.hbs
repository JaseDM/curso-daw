    {{> header }}

    <!-- Content Wrapper -->
    <div class="shop-wrapper has-error">

        <!-- Search overlay -->
        <div class="search-overlay"></div>

        <!-- Search input -->
        <div class="search-input-wrapper is-desktop is-hidden">
            <div class="field">
                <div class="control">
                    <input id="nephos-search" type="text" name="search" placeholder="Search a Product">
                    <span id="clear-search" role="button"><i data-feather="x"></i></span>
                    <span class="bar"></span>
                </div>
                <span class="search-help">Type the name of the product you are looking for</span>
            </div>
        </div>
        <div class="error-wrapper">
            <div class="error-code">
                404
            </div>
            <img src="assets/img/illustrations/error1.svg" alt="">
            <h2>Page not found.</h2>
            <p>Sorry, we couldn't find that page. Continue browsing or go back to homepage.</p>
            <a href="/" class="button is-solid primary-button raised is-rounded">Back To Home</a>
        </div>
    </div>
    <!-- /Content Wrapper -->
    <!-- Concatenated plugins -->
    <script src="assets/js/app.js"></script>

    <!--Data-->
    <script src="assets/js/_data-wishlist.js"></script>
    <script src="assets/js/_data-addresses.js"></script>
    <script src="assets/js/_data-orders.js"></script>

    <!-- Nephos js -->
    <script src="assets/js/functions.js"></script>
    <script src="assets/js/nephos.js"></script>
    <script src="assets/js/authentication.js"></script>
    <script src="assets/js/cart.js"></script>

    <!--Pages-->
    <script src="assets/js/account.js"></script>
    <script src="assets/js/wishlist.js"></script>
    <script src="assets/js/product.js"></script>
    <script src="assets/js/orders.js"></script>
    <script src="assets/js/order.js"></script>
    <script src="assets/js/checkout.js"></script>
    <script src="assets/js/search.js"></script>
</body>

</html>